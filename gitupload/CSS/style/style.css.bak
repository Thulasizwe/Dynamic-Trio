ul {
    list-style: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
}

ul li {
    float: left;
    margin-right: 10px;
}

ul a {
    text-transform: uppercase;
    color: #fff;
    font-size: 14px;
    padding: 10px 15px;
    line-height: 18px;
    text-decoration: none;
    border-radius: 20px;
    display: block;
}

ul a:hover {
    background: rgba(0,0,0,0.2);
}
ul a:active, ul a.active {
    background: rgba(0,0,0,0.3);
